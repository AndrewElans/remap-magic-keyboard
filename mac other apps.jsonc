                    "mandatory": [
                        "command"
                    ]
                }
            },
            "to": [
                {
                    "key_code": "vk_none"
                }
            ],
            "type": "basic"
        },
        { // vscode cmd+Q -> Lcmd+Rcmd+Q
            "conditions": [
                {
